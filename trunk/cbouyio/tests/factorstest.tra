transsys faCtorstest
# A simple transsys program for testing the lattice interation percentage.
{
  factor FACTOR_A
  {
    decay: 0.5;
    diffusibility: 0.0;
  }
  
  factor FACTOR_B
  {
    decay: 0.5;
    diffusibility: 0.0;
  }
  
  gene Gene_A
  {
    promoter
    {
      constitutive: 0.5;
      FACTOR_B: repress(0.5, 0.5);
    }
    product
    {
      default: FACTOR_A;
    }
  }

  gene Gene_B
  {
    promoter
    {
      constitutive: 0.5;
      FACTOR_A: activate(0.5, 0.5);
    }
    product
    {
      default: FACTOR_B;
    }
  }
}
