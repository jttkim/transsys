transsys timestepstest
# A transsys program for testing timestep mechanism and printout.
{
  factor TIMESTEP_A
  {
    decay: 0.5;
    diffusibility: 0.0;
  }
  
  factor TIMESTEP_B
  {
    decay: 0.5;
    diffusibility: 0.0;
  }
  
  gene timestep_A
  {
    promoter
    {
      constitutive: 0.5;
      TIMESTEP_B: repress(0.5, 0.5);
    }
    product
    {
      default: TIMESTEP_A;
    }
  }

  gene timestep_B
  {
    promoter
    {
      constitutive: 0.5;
      TIMESTEP_A: activate(0.5, 0.5);
    }
    product
    {
      default: TIMESTEP_B;
    }
  }
}
