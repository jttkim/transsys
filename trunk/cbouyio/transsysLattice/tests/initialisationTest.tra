transsys initialisationTest

# A transsys program for testing the module lattice initialisation facilities.
# The same transsys program is used to test the random Gauss and the random
# Uniform initialisation procedures. 

{
  factor FACTOR_A
  {
    decay: 0.2;
    diffusibility: 0.1;
  }
  
  factor FACTOR_B
  {
    decay: 0.2;
    diffusibility: 0.1;
  }
  
  gene Gene_A
  {
    promoter
    {
      constitutive: 0;
      FACTOR_B: repress(0.5, 1);
    }
    product
    {
      default: FACTOR_A;
    }
  }

  gene Gene_B
  {
    promoter
    {
      constitutive: 0;
      FACTOR_A: activate(0.5, 1);
    }
    product
    {
      default: FACTOR_B;
    }
  }
}
