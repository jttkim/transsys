transsys sFactorTest

# A transsys program for testing the module signal factor mechanism.
# This transsys program implements a few more transsys attributes
# (diffusibiulity, constitutive).

{
  factor FACTOR_A
  {
    decay: 0.5;
    diffusibility: 0.5;
  }
  
  factor FACTOR_B
  {
    decay: 0.5;
    diffusibility: 0.5;
  }
  
  gene Gene_A
  {
    promoter
    {
      constitutive: 1;
      FACTOR_B: repress(1, 1);
    }
    product
    {
      default: FACTOR_A;
    }
  }

  gene Gene_B
  {
    promoter
    {
      constitutive: 1;
      FACTOR_A: activate(1, 1);
    }
    product
    {
      default: FACTOR_B;
    }
  }
}
