transsys intertest
# A simple transsys program for testing the lattice interation percentage.
{
  factor INTERTEST
  {
    decay: 0.5;
    diffusibility: 0.0;
  }
  gene intertest
  {
    promoter
    {
      constitutive: 0.5;
      INTERTEST: repress(0.1, 1.5);
    }
    product
    {
      default: INTERTEST;
    }
  }
}
